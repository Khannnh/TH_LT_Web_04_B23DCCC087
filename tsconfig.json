{
	"compilerOptions": {
		"outDir": "build/dist",
		"module": "esnext",
		"target": "esnext",
		"lib": ["esnext", "dom"],
		"sourceMap": true,
		"baseUrl": ".",
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"allowJs": true,
		"skipLibCheck": true,
		"experimentalDecorators": true,
		"strict": true,
		"paths": {
			"@/*": ["src/*"],
			"@@/*": ["./src/.umi/*"],
			"react": ["./node_modules/@types/react"]
		},
		"typeRoots": ["./src/types", "./node_modules/@types"]
	},
	"include": [
		"mock/**/*",
		"src/**/*",
		"tests/**/*",
		"test/**/*",
		"__test__/**/*",
		"typings/**/*",
		"config/**/*",
		".eslintrc.js",
		".stylelintrc.js",
		".prettierrc.js",
		"mock/*",
		"typings.d.ts"
, "../base-web-umi/src/components/StaffForm", "../base-web-umi/src/models/useStaff.ts", "../base-web-umi/src/pages/StaffManagement", "../base-web-umi/src/services/staff.ts", "../base-web-umi/src/models/staff.ts"	],
	"exclude": ["node_modules", "build", "dist", "scripts", "src/.umi/*", "webpack", "jest"]
}
